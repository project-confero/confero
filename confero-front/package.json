{
  "name": "confero-front",
  "version": "0.1.0",
  "private": false,
  "license": "MIT",
  "dependencies": {
    "@material-ui/core": "^4.9.0",
    "@types/d3": "^5.7.2",
    "@types/lodash": "^4.14.149",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@types/react-router-dom": "^5.1.3",
    "axios": "^0.21.1",
    "d3": "^5.15.0",
    "d3-force": "^2.0.1",
    "lodash": "^4.17.15",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.3.0",
    "typescript": "~3.7.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build && cp ./build/index.html ./build/200.html",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "format": "npm-run-all -c format:*",
    "format:eslint": "eslint --fix '**/*.ts'",
    "format:prettier": "prettier --write '**/*.{tsx,ts,json,md}'",
    "lint": "npm-run-all -c lint:*",
    "lint:eslint": "eslint '**/*.ts'",
    "lint:prettier": "prettier --check '**/*.{tsx,ts,json,md}'",
    "deploy": "surge ./build --domain https://confero.surge.sh"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.0.2",
    "@testing-library/react": "^9.4.0",
    "@testing-library/user-event": "^8.0.5",
    "@types/enzyme": "^3.10.4",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/jest": "^24.0.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint-config-prettier": "^6.9.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "surge": "^0.21.3"
  }
}
